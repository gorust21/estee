<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.estee.dao.MainMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.estee.entity.Main">
                    <id column="id" property="id"/>
                    <result column="scene_num" property="sceneNum"/>
                    <result column="title" property="title"/>
                    <result column="about" property="about"/>
                    <result column="adapter" property="adapter"/>
                    <result column="create_time" property="createTime"/>
                    <result column="update_time" property="updateTime"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
            id, title, about, adapter, create_time, update_time
        </sql>

</mapper>